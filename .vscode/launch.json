{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Debug With Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:37876",
            "webRoot": "${workspaceRoot}/src",
            "userDataDir": "${workspaceRoot}/.chrome",
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            }
        }
    ]
}